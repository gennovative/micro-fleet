{
  "name": "@micro-fleet/common-contracts",
  "version": "1.2.2",
  "description": "Contains interfaces, models and constants which are shared between micro services.",
  "scripts": {
    "clean": "rimraf dist",
    "dedup": "rimraf node_modules/**/node_modules/**/typings",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "compile:ts": "tsc",
    "watch:ts": "tsc -w",
    "test": "nyc mocha"



  },
  "dependencies": {
    "automapper-ts": "~1.8",
    "joi": "~13.0"
  },
  "peerDependencies": {
    "@micro-fleet/common-util": "~1.2"
  },

  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },

  "author": {
    "name": "gennovative",
    "url": "https://github.com/gennovative"
  },
  "engines": {
    "node": "^8",
    "npm": "^5"
  },
  "license": "MIT",
  "private": false,
  "main": "./dist/app/index.js",
  "types": "./typings/app.d.ts"
}

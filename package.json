	},
	"devDependencies": {
		"@types/debug": "~0.0.30",
		"@types/node": "~10.14",
		"chalk": "~2.4",
		"fs-extra": "~6.0",
		"ts-declarator": "~1.0",
		"typescript": "~3.3"
	},
	"author": {
		"name": "gennovative",
		"url": "https://github.com/gennovative"
	},

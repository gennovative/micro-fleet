{
	"name": "micro-fleet",
	"version": "2.3.0-rc1",
	"description": "Spin up your fleet of containerized microservices with the power of TypeScript.",
	"scripts": {
		"clean": "lerna run clean",
		"lint": "lerna run lint",
		"compile": "lerna run compile",
		"test": "lerna run test",
		"dedup": "lerna run dedup",
		"definition": "lerna run definition",
